var page = document.getElementById('page');
$(document).ready(function(){
	$.ajax({
        url : url + '/Includes/detail/Profil/Groupe',
        type : 'GET',
        data : 'item.id=' + item,
        success : function(html, statut){
            if(html){
                $('#profil').html(html);
            }else{
                $('#profil').load(url.value + '/Includes/Loader');
            }
        },
        error : function(resultat, statut, erreur){
        }
    });
});

