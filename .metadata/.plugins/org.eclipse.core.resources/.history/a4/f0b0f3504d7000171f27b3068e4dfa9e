var page = document.getElementById('page');
$(document).ready(function(){
	$.ajax({
        url : url.value+'/Includes/groupeDetail/Profil',
        type : 'GET',
        data : 'groupe.id=' + groupe,
        success : function(html, statut){
            if(html){
                $('#profil tbody').html(html);
            }else{
                $('#details .modal-body').load('Waiter');
            }
        },
        error : function(resultat, statut, erreur){
        }
    });
	$.ajax({
        url : url.value+'/Includes/groupeDetail/Utilisateur',
        type : 'GET',
        data : 'groupe.id=' + groupe,
        success : function(html, statut){
            if(html){
                $('#profilSelect').html(html);
            }else{
                $('#details tbody').load('Waiter');
            }
        },
        error : function(resultat, statut, erreur){
        }
    });
	$.ajax({
        url : url.value+'/Includes/groupeDetail/Agent',
        type : 'GET',
        data : 'groupe.id=' + groupe ,
        success : function(html, statut){
            if(html){
                $('#agent tbody').html(html);
            }else{
                $('#agent tbody').load('Waiter');
            }
        },
        error : function(resultat, statut, erreur){
        }
    });
});

